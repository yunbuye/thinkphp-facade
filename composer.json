{
    "name": "yunbuye/thinkphp-facade",
    "license": "MIT",
    "description": "ThinkPHP 方便被 mock 的 facade",
    "keywords": [
        "facade",
        "ThinkPHP",
        "mockery",
        "mock"
    ],
    "homepage": "http://github.com/yunbuye/thinkphp-facade",
    "authors": [
        {
            "name": "xwpd",
            "email": "514261300@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "6.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Yunbuye\\ThinkFacade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yunbuye\\ThinkFacade\\Tests\\": "tests/",
            "think\\":"vendor/topthink/framework/library/think/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias":{
            "dev-master":"0.x.x-dev"
        },
        "think": {
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
